    <style type="text/css">
    #preloader {
      position: fixed;
      top: 0;
      left: 0;
      background-color: #2A3F54;
    }
    #preloader img {
      position: fixed;
      z-index: 1000000;
      left: 50%;
      top: 50%;
      margin-left: -100px;
      max-width: 100%;
      margin-top: -100px;

    }
  </style>
  <div id="preloader" >
  <img  src="/preloader.gif">
  </div>
  <body class="nav-md" >
